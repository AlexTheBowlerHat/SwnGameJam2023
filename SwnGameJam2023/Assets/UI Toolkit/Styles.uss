.Background-Aligner {
    width: 100%;
    height: 100%;
    justify-content: space-between;
    align-items: stretch;
    flex-direction: row;
}

.default-block {
    height: 424px;
    width: 374px;
    background-color: rgb(93, 36, 132);
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    align-items: center;
    justify-content: flex-start;
    flex-direction: column;
    flex-wrap: wrap;
    flex-shrink: 1;
    margin-top: 0;
    margin-bottom: 0;
    -unity-text-align: middle-center;
    top: 10px;
}

.label-default {
    height: 59px;
    position: relative;
    bottom: auto;
    -unity-font-style: bold;
    -unity-font: url('project://database/Assets/UI%20Toolkit/Fonts/RussoOne-Regular.ttf?fileID=12800000&guid=feb766379d98cfb4e85ae694393e882e&type=3#RussoOne-Regular');
